{
  "name": "@clawbureau/clawsig-sdk",
  "version": "0.3.3",
  "description": "Lightweight SDK for emitting verifiable proof bundles from any Node.js agent â€” model calls, tool receipts, side-effect receipts, human approvals, and capability negotiation.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./preload": {
      "import": "./src/preload.mjs"
    },
    "./node-preload-sentinel": {
      "import": "./src/node-preload-sentinel.mjs"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "dist/**/*.js.map",
    "src/preload.mjs",
    "src/node-preload-sentinel.mjs",
    "src/sentinel-shell.sh",
    "src/sentinels/interpose/libclawsig_interpose.c",
    "src/sentinels/interpose/Makefile",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "smoke:node": "npm run build && node scripts/smoke-node.mjs"
  },
  "keywords": [
    "clawsig",
    "proof-bundle",
    "proof-of-harness",
    "agent-verification",
    "tool-receipts",
    "side-effect-receipts",
    "Ed25519",
    "DID"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/clawbureau/clawbureau",
    "directory": "packages/clawsig-sdk"
  },
  "homepage": "https://www.clawea.com/docs#sdk",
  "bugs": "https://github.com/clawbureau/clawbureau/issues",
  "devDependencies": {
    "@types/node": "^25.2.1",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=20"
  },
  "license": "MIT"
}
