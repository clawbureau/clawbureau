{
  "name": "@openclaw/provider-clawproxy",
  "version": "0.1.0",
  "description": "OpenClaw provider plugin that routes model calls through clawproxy with run bindings for automatic receipts",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "openclaw": {
    "extensions": ["./src/openclaw.ts"],
    "slots": ["provider"]
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "npm run build && node --test test/*.test.mjs"
  },
  "peerDependencies": {
    "openclaw": ">=2026.1.26"
  },
  "devDependencies": {
    "typescript": "^5.7.0"
  },
  "files": [
    "dist",
    "presets",
    "openclaw.plugin.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/clawbureau/monorepo.git",
    "directory": "packages/openclaw-provider-clawproxy"
  },
  "keywords": [
    "openclaw",
    "clawproxy",
    "clawsig",
    "provider",
    "proof-of-harness"
  ],
  "license": "MIT"
}
